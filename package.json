{
  "name": "blog",
  "description": "Kylie Stewart's online blog",
  "version": "3.0.1",
  "author": "Kylie Stewart <kylie@kyliestewart.tech>",
  "bugs": {
    "url": "https://github.com/kale-stew/blog/issues"
  },
  "scripts": {
    "build": "gatsby clean && gatsby build --prefix-paths",
    "test-build": "yarn run build && gatsby serve",
    "deploy": "gatsby clean && gatsby build && gh-pages -b master -d public",
    "dev": "gatsby develop",
    "develop": "gatsby develop",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*/**.{js,jsx,scss}' 'gatsby-*.js' 'content/**/*.md'",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "new-post": "node ./cli/create-new-post.js"
  },
  "dependencies": {
    "gatsby": "^2.2.8",
    "gatsby-image": "^2.0.34",
    "gatsby-plugin-feed": "^2.1.0",
    "gatsby-plugin-google-analytics": "^2.0.17",
    "gatsby-plugin-lodash": "^3.0.5",
    "gatsby-plugin-manifest": "^2.0.24",
    "gatsby-plugin-offline": "^3.2.18",
    "gatsby-plugin-react-helmet": "^3.0.10",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-sharp": "^2.0.30",
    "gatsby-plugin-sitemap": "^2.0.10",
    "gatsby-plugin-typography": "^2.2.10",
    "gatsby-remark-autolink-headers": "^2.0.16",
    "gatsby-remark-copy-linked-files": "^2.0.11",
    "gatsby-remark-emoji": "0.0.3",
    "gatsby-remark-images": "^3.0.10",
    "gatsby-remark-images-medium-zoom": "^1.7.0",
    "gatsby-remark-katex": "^3.1.25",
    "gatsby-remark-prismjs": "^3.2.6",
    "gatsby-remark-responsive-iframe": "^2.1.1",
    "gatsby-remark-smartypants": "^2.0.9",
    "gatsby-source-filesystem": "^2.0.28",
    "gatsby-transformer-remark": "^2.3.8",
    "gatsby-transformer-sharp": "^2.1.17",
    "intersection-observer": "^0.11.0",
    "katex": "^0.12.0",
    "lodash.partial": "^4.2.1",
    "lodash.uniq": "^4.5.0",
    "node-sass": "^4.11.0",
    "prismjs": "^1.15.0",
    "react": "^16.10.2",
    "react-animations": "^1.0.0",
    "react-dom": "^16.10.2",
    "react-helmet": "^6.1.0",
    "react-scripts": "3.4.1",
    "react-social-icons": "^4.1.0",
    "react-switch": "^5.0.0",
    "react-typography": "^0.16.19",
    "smooth-scroll": "^16.0.0",
    "typeface-fira-sans": "^0.0.75",
    "typography": "^0.16.17",
    "typography-theme-github": "^0.16.18"
  },
  "devDependencies": {
    "date-fns": "^2.15.0",
    "eslint": "^7.5.0",
    "eslint-plugin-react": "^7.11.1",
    "fs-extra": "^9.0.1",
    "gatsby-post-gen": "^1.0.0",
    "gh-pages": "^3.1.0",
    "gray-matter": "^4.0.2",
    "inquirer": "^7.3.2",
    "prettier": "^2.0.5",
    "recursive-readdir": "^2.2.2",
    "signale": "^1.3.0"
  },
  "keywords": [
    "gatsby",
    "blog"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kale-stew/blog.git"
  }
}
